{
  "name": "skillio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "emulators": "firebase emulators:start",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:studio": "drizzle-kit studio",
    "db:migration": "drizzle-kit generate:pg --out db/migrations --schema db/schema.ts",
    "db:push": "drizzle-kit push:pg",
    "db:drop": "drizzle-kit drop --out db/migrations"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.6",
    "drizzle-zod": "^0.5.1",
    "lucide-react": "^0.363.0",
    "next": "14.1.4",
    "pg": "^8.11.4",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/pg": "^8.11.4",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "firebase-tools": "^13.6.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
